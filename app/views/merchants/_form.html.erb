<%= simple_form_for(@merchant) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.label "Add Logo" %>
    <%= f.file_field :logo, required: false, class: "form-control" %><br>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
